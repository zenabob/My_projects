/**
 * 8-way demultiplexor:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;
    
    PARTS:
    // Put your code here:
    DMux(in=in,sel=sel[2],a=q,b=r);
    DMux(in=q,sel=sel[1],a=k , b=l);
    DMux(in=r,sel=sel[1],a=m , b=n);
    DMux(in=k,sel=sel[0],a=a,b=b);
    DMux(in=l,sel=sel[0],a=c, b=d);
    DMux(in=m,sel=sel[0],a=e , b=f);
    DMux(in=n,sel=sel[0],a=g, b=h);



}